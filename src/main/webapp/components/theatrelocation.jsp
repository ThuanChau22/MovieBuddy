<form id="theatreLocationForm" action="#" method="POST" class="form-inline">
    <div class="input-group input-group-sm">
        <div class="input-group-prepend">
            <span class="input-group-text"><b>Theatrename</b></span>
        </div>
        <input class="form-control" type="text" placeholder="Zip Code" value="${zipcode}"
        style="max-width: 100px;">
        <div class="input-group-append">
            <input type="submit" class="btn btn-outline-info" value="Change">
        </div>
    </div>
</form>